<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="frontend.controllers.LoginController" stylesheets="@login-view.css">

    <Button 
        fx:id="googleSignInButton" 
        onAction="#onLoginWithGoogle" 
        styleClass="googleSignInButton"
    />

    <Button
        text="Cancel"
        fx:id="cancelLogin"
        onAction="#cancelLogin"
        styleClass="cancelLoginButton"
        visible="false"
    />
</VBox>
